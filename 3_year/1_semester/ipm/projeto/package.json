    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.5.3",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "concurrently": "^8.2.0",
    "json-server": "^0.17.4",
    "vite": "^4.0.0"
  }
}
